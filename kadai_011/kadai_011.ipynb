      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for分\n",
        "for n in [0,1,2]:\n",
        "    if n == 2:\n",
        "        break\n",
        "    for value in array:\n",
        "        print(value)"
      ]
